{
	"name": "@shinnn/eslint-config",
	"version": "6.2.1",
	"description": "shinnn's ESLint config for both client-side and Node.js-based projects",
	"repository": "shinnn/eslint-config",
	"scripts": {
		"pretest": "eslint .",
		"test": "nyc --reporter=html --reporter=text node test/test.js"
	},
	"license": "ISC",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"index.js"
	],
	"keywords": [
		"rc",
		"setting",
		"config",
		"configuration",
		"json",
		"validate",
		"lint",
		"linter",
		"eslint",
		"fix",
		"autofix",
		"codeframe",
		"cache",
		"env",
		"rule",
		"syntax",
		"style",
		"window",
		"document",
		"personal"
	],
	"dependencies": {
		"eslint-plugin-no-use-extend-native": "^0.3.12",
		"eslint-plugin-node": "^6.0.1",
		"eslint-plugin-promise": "^3.8.0",
		"is-resolvable": "^1.1.0",
		"lodash": "^4.17.10"
	},
	"devDependencies": {
		"chalk": "^2.4.1",
		"eslint": "^5.1.0",
		"nyc": "^12.0.2",
		"ora": "^2.1.0",
		"p-event": "^2.1.0",
		"unconfigured-eslint-rules": "^6.0.0"
	},
	"eslintConfig": {
		"extends": "./index.js"
	},
	"eslintIgnore": [
		"test/fixtures*"
	]
}
